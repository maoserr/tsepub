{
  "name": "tsepub",
  "description": "Typescript Epub generator library",
  "version": "1.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "watch": "tsc -w --sourcemap",
    "compile": "node compile_template.js",
    "build": "npm run compile && tsc",
    "package": "npm run build && npm pack",
    "test": "karma start",
    "all": "npm run package && npm test"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/maoserr/tsepub.git"
  },
  "keywords": [
    "epub",
    "typescript",
    "ebook",
    "kindle"
  ],
  "author": "Mao Yu<maoserr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maoserr/tsepub/issues"
  },
  "homepage": "https://github.com/maoserr/tsepub#readme",
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/uuid": "^9.0.1",
    "jasmine-core": "^4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.1.0",
    "karma-typescript": "^5.5.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  }
}
