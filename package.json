{
  "name": "tsepub",
  "description": "Typescript Epub generator library",
  "version": "1.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "watch": "tsc -w --sourcemap",
    "compile": "node compile_template.mjs",
    "build": "npm run compile && tsc",
    "package": "npm run build && npm pack",
    "test": "mocha",
    "int_test": "karma start",
    "all": "npm run package && npm test"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maoserr/tsepub.git"
  },
  "keywords": [
    "epub",
    "typescript",
    "ebook",
    "kindle"
  ],
  "author": "Mao Yu<maoserr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maoserr/tsepub/issues"
  },
  "homepage": "https://github.com/maoserr/tsepub#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.1",
    "chai": "^5.0.0",
    "global-jsdom": "9.2.0",
    "jsdom": "23.1.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.4",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  }
}
